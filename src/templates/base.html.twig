<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Analyse CDR{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
            <link rel="stylesheet" href="{{ asset('/build/css/date-picker/daterangepicker.css') }}" />
            <link rel="stylesheet" href="{{ asset('/build/css/bootstrap.css') }}" />
            <link rel="stylesheet" href="{{ asset('/build/css/icon/all.css') }}" />
            <link rel="stylesheet" href="{{ asset('/build/css/icon/fontawesome.css') }}" />
            <link rel="stylesheet" href="{{ asset('/build/css/style.css') }}" />
            <style>
                #sigma-parent {
                    width: 100%;
                    height: 900px;
                    position: relative;
                }

                #sigma-container{
                    position: absolute;
                    width: 100%;
                    height: 100%;
                }

            </style>
        {% endblock %}


        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
            <script  src="{{ asset('/build/js/jquery/jquery.js') }}"></script>
            <script  src="{{ asset('/build/js/bootstrap.js') }}"></script>
            <script src="{{ asset('/build/js/vue/vue.js') }}"></script>
            <script src="{{ asset('/build/js/vue/axios.min.js') }}"></script>
            <script src="{{ asset('/build/js/uikit/uikit.js') }}"></script>
            <script  src="{{ asset('/build/js/date-picker/moment.min.js') }}"></script>
            <script  src="{{ asset('/build/js/date-picker/daterangepicker.js') }}"></script>


        {% endblock %}
        {% block add_js %}

        {% endblock %}
    </head>
    <body>

        <div class="container-fluid">

        </div>
        {% block body %}
        {% endblock %}

    </body>
    {% block bottom_js %}
        <script  src="{{ asset('/build/js/custom/script.js') }}"></script>
    {% endblock %}
</html>
